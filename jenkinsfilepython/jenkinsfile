pipeline {
    agent any
    stages{
        stage('Fetch code'){
            steps{
                git credentialsId: 'BitBucket-Id', url: '${params.URL_BITBUCKET}', branch: 'master'
            }
        }
        
        stage('Build'){
            steps{
                sh 'pyinstaller --onefile relatorio.py --name=relatorioLeite'
            }
        }
    }
}