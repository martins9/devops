pipeline {
    agent any
    stages{
        stage('Fetch code'){
            steps{
                echo ${params.URL_BITBUCKET}
                git credentialsId: 'BitBucket-Id', branch: '${params.BRANCH}', url: '${params.URL_BITBUCKET}'
            }
        }
        
        stage('Build'){
            steps{
                sh 'pyinstaller --onefile relatorio.py --name=relatorioLeite'
            }
        }
    }
}